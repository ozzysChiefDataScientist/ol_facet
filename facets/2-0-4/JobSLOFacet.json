{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/ozzysChiefDataScientist/ol_facet/main/facets/2-0-4/JobSLOFacet.json",
  "$defs": {
    "JobSLOFacet": {
      "allOf": [
        {
          "$ref": "https://openlineage.io/spec/2-0-2/OpenLineage.json#/$defs/JobFacet"
        },
        {
          "type": "object",
          "properties": {
            "slo_name": {
              "type": "string",
              "example": "BATCH"
            },
            "product": {
              "type": "string",
              "example": "SPARK"
            }
          },
          "required": ["slo_name", "product"]
        }
      ],
      "type": "object"
    }
  },
  "type": "object"
}
